<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="styles"/>
    <update handle="editor"/>
    <body>
        <referenceContainer name="page.main.actions">
            <block class="Magento\Backend\Block\Store\Switcher" name="store_switcher" template="Magento_Backend::store/switcher.phtml"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Eleanorsoft\DesignersPage\Block\Url" name="url_provider" template="Eleanorsoft_DesignersPage::url.phtml"/>
            <uiComponent name="designers_grid_form"/>
        </referenceContainer>
    </body>
</page>
